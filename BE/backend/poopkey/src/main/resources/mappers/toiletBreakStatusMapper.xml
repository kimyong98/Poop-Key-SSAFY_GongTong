<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.poopkey.model.dao.ToiletBreakStatusDao">
        <select id="selectOne" parameterType="long" resultType="ToiletBreakStatus">
            SELECT stall_id AS stallId, status FROM stall WHERE stall_id = #{stallId}
        </select>

</mapper>